{
  "name": "pwned",
  "version": "1.0.9-dev.0",
  "description": "A command-line tool for querying the 'Have I been pwned?' service.",
  "keywords": [
    "haveibeenpwned",
    "hibp",
    "pwned",
    "security",
    "hack",
    "dump",
    "breach",
    "pastes",
    "passwords"
  ],
  "author": "Justin Hall <justin.r.hall@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "main": "bin/pwned.js",
  "bin": {
    "pwned": "./bin/pwned.js"
  },
  "directories": {
    "bin": "./bin",
    "lib": "./lib"
  },
  "scripts": {
    "clean": "npm run srcClean && npm run testClean",
    "compile": "npm run clean && npm run srcCompile && npm run testCompile",
    "coverage": "npm run compile && nyc mocha --recursive lib-test",
    "lint": "eslint bin src test",
    "prepublish": "npm run srcClean && npm run srcCompile",
    "srcClean": "rimraf ./lib",
    "srcCompile": "babel src --out-dir lib --source-maps",
    "test": "npm run coverage",
    "testClean": "rimraf ./lib-test ./coverage",
    "testCompile": "babel test --out-dir lib-test --source-maps"
  },
  "nyc": {
    "all": true,
    "include": [
      "**/lib/**"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wKovacs64/pwned.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/pwned/issues"
  },
  "homepage": "https://github.com/wKovacs64/pwned",
  "engines": {
    "iojs": "*",
    "node": ">= 0.10"
  },
  "dependencies": {
    "cli-spinner": "^0.2.5",
    "commander": "^2.9.0",
    "es6-promise": "^3.2.1",
    "hibp": "^1.0.7",
    "prettyjson": "^1.1.3",
    "source-map-support": "^0.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "eslint": "^3.1.1",
    "eslint-config-semistandard": "^6.0.2",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "expect.js": "^0.3.1",
    "mocha": "^2.5.3",
    "mockery": "^1.7.0",
    "nyc": "^7.1.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.4"
  },
  "preferGlobal": true
}
