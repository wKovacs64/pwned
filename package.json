{
  "name": "pwned",
  "version": "6.3.1-dev.0",
  "description": "A command-line tool for querying the 'Have I been pwned?' service.",
  "keywords": [
    "haveibeenpwned",
    "hibp",
    "pwned",
    "security",
    "hack",
    "dump",
    "breach",
    "pastes",
    "passwords"
  ],
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "main": "bin/pwned.js",
  "bin": "bin/pwned.js",
  "directories": {
    "bin": "./bin",
    "lib": "./lib"
  },
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "build": "babel src --extensions .js,.ts --out-dir lib --source-maps --ignore \"**/*.test.ts\",\"**/*.test.js\",\"**/__mocks__/\"",
    "check-types": "tsc",
    "clean": "rimraf lib coverage",
    "format": "prettier --write \"**/*.{js,ts,json,md,yml}\"",
    "lint": "eslint --ext \".js,.ts\" .",
    "prebuild": "npm run --silent clean",
    "prepublishOnly": "npm-run-all --silent --sequential lint check-types test build",
    "test": "cross-env NODE_ENV=test jest",
    "validate": "npm-run-all --silent --parallel lint check-types"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wKovacs64/pwned.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/pwned/issues"
  },
  "homepage": "https://wkovacs64.github.io/pwned",
  "engines": {
    "node": ">= 6.4.0"
  },
  "dependencies": {
    "hibp": "^7.5.0",
    "ora": "^3.0.0",
    "prettyjson": "^1.2.1",
    "source-map-support": "^0.5.10",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/parser": "7.3.2",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-typescript": "7.1.0",
    "@types/jest": "23.3.13",
    "@types/node": "10.12.21",
    "@types/ora": "3.0.0",
    "@types/prettyjson": "0.0.28",
    "@types/yargs": "12.0.8",
    "@typescript-eslint/eslint-plugin": "1.3.0",
    "@wkovacs64/eslint-config-ts": "2.0.0",
    "babel-jest": "24.1.0",
    "codecov": "3.1.0",
    "cross-env": "5.2.0",
    "eslint": "5.13.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jest": "22.2.2",
    "husky": "1.3.1",
    "jest": "24.1.0",
    "lint-staged": "8.1.3",
    "npm-run-all": "4.1.5",
    "prettier": "1.16.4",
    "rimraf": "2.6.3",
    "typescript": "3.3.3"
  }
}
