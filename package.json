{
  "name": "pwned",
  "version": "0.0.0-semantically-released",
  "description": "A command-line tool for querying the 'Have I been pwned?' service.",
  "keywords": [
    "haveibeenpwned",
    "hibp",
    "pwned",
    "security",
    "hack",
    "dump",
    "breach",
    "pastes",
    "passwords"
  ],
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "main": "bin/pwned.js",
  "bin": "bin/pwned.js",
  "directories": {
    "bin": "./bin",
    "lib": "./lib"
  },
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "build": "babel src --extensions .js,.ts --out-dir lib --source-maps --ignore \"**/*.test.ts\",\"**/*.test.js\",\"**/__mocks__/\"",
    "check-types": "tsc",
    "clean": "rimraf lib coverage",
    "cm": "git-cz",
    "format": "prettier --write \"**/*.{js,ts,tsx,html,json,md,yml,yaml}\"",
    "lint": "eslint --ext \".js,.ts,.tsx\" .",
    "prebuild": "npm run --silent clean",
    "prepublishOnly": "npm-run-all --silent --sequential lint check-types test build",
    "test": "cross-env NODE_ENV=test jest",
    "validate": "npm-run-all --silent --parallel lint check-types"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/pwned.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/pwned/issues"
  },
  "homepage": "https://wkovacs64.github.io/pwned",
  "engines": {
    "node": ">= 8"
  },
  "dependencies": {
    "common-tags": "^1.8.0",
    "conf": "^5.0.0",
    "hibp": "^8.0.0",
    "ora": "^3.4.0",
    "prettyjson": "^1.2.1",
    "source-map-support": "^0.5.12",
    "yargs": "^14.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.5.5",
    "@babel/core": "7.5.5",
    "@babel/parser": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-typescript": "7.3.3",
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-conventional": "8.1.0",
    "@types/common-tags": "1.8.0",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.2",
    "@types/ora": "3.2.0",
    "@types/prettyjson": "0.0.29",
    "@types/yargs": "13.0.2",
    "@wkovacs64/prettier-config": "1.0.2",
    "babel-jest": "24.9.0",
    "codecov": "3.5.0",
    "commitizen": "4.0.3",
    "cross-env": "5.2.0",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "6.2.1",
    "eslint-plugin-wkovacs64": "2.0.1",
    "husky": "3.0.4",
    "jest": "24.9.0",
    "lint-staged": "9.2.3",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "rimraf": "3.0.0",
    "semantic-release": "15.13.24",
    "typescript": "3.5.3"
  }
}
