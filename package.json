{
  "name": "pwned",
  "version": "0.0.0-semantically-released",
  "description": "A command-line tool for querying the 'Have I been pwned?' service.",
  "keywords": [
    "haveibeenpwned",
    "hibp",
    "pwned",
    "security",
    "hack",
    "dump",
    "breach",
    "pastes",
    "passwords"
  ],
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "type": "module",
  "main": "bin/pwned.js",
  "bin": "bin/pwned.js",
  "directories": {
    "bin": "./bin",
    "lib": "./lib"
  },
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "build": "babel src --extensions .js,.ts --out-dir lib --source-maps --ignore \"**/*.test.ts\",\"**/*.test.js\",\"**/__mocks__/\"",
    "check-types": "tsc",
    "clean": "rimraf lib coverage",
    "cm": "git-cz",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,html,css,json,md,mdx,yml,yaml}\"",
    "lint": "eslint --ext \".js,.jsx,.ts,.tsx\" .",
    "husky:install": "husky install",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "prebuild": "npm run --silent clean",
    "prepublishOnly": "npm-run-all --silent --sequential lint check-types test build",
    "prepare": "npm run --silent husky:install",
    "validate": "npm-run-all --silent --parallel lint check-types"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wKovacs64/pwned.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/pwned/issues"
  },
  "homepage": "https://wkovacs64.github.io/pwned",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "dependencies": {
    "common-tags": "^1.8.0",
    "conf": "^10.0.0",
    "fs-extra": "10.1.0",
    "hibp": "^11.0.0",
    "ora": "^6.0.0",
    "prettyjson": "^1.2.1",
    "source-map-support": "^0.5.12",
    "yargs": "^17.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/core": "7.17.9",
    "@babel/parser": "7.17.9",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-typescript": "7.16.7",
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@types/common-tags": "1.8.1",
    "@types/node": "16.11.29",
    "@types/prettyjson": "0.0.30",
    "@types/ws": "8.5.3",
    "@types/yargs": "17.0.10",
    "@wkovacs64/prettier-config": "3.0.1",
    "c8": "7.11.2",
    "codecov": "3.8.3",
    "commitizen": "4.2.4",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-plugin-wkovacs64": "13.13.0",
    "husky": "7.0.4",
    "lint-staged": "12.4.0",
    "msw": "0.39.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "semantic-release": "19.0.2",
    "typescript": "4.6.3",
    "vitest": "0.10.0"
  }
}
