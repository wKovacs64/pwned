{
  "name": "pwned",
  "version": "4.0.0-dev.0",
  "description": "A command-line tool for querying the 'Have I been pwned?' service.",
  "keywords": [
    "haveibeenpwned",
    "hibp",
    "pwned",
    "security",
    "hack",
    "dump",
    "breach",
    "pastes",
    "passwords"
  ],
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "main": "bin/pwned.js",
  "bin": "bin/pwned.js",
  "directories": {
    "bin": "./bin",
    "lib": "./lib"
  },
  "scripts": {
    "build": "babel src --out-dir lib --source-maps",
    "clean": "rimraf lib coverage",
    "format": "prettier-eslint --write \"{bin,src,test}/**/*.js\"",
    "lint": "eslint bin src test",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "test": "cross-env NODE_ENV=test jest --config jest.config.json",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wKovacs64/pwned.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/pwned/issues"
  },
  "homepage": "https://wkovacs64.github.io/pwned",
  "engines": {
    "node": ">= 4"
  },
  "dependencies": {
    "cli-spinner": "^0.2.6",
    "hibp": "^5.2.0",
    "prettyjson": "^1.2.1",
    "source-map-support": "^0.5.0",
    "yargs": "^10.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^21.2.1",
    "prettier-eslint-cli": "^4.4.0",
    "rimraf": "^2.6.2"
  }
}
