{
  "name": "pwned",
  "version": "6.1.2-dev.0",
  "description": "A command-line tool for querying the 'Have I been pwned?' service.",
  "keywords": [
    "haveibeenpwned",
    "hibp",
    "pwned",
    "security",
    "hack",
    "dump",
    "breach",
    "pastes",
    "passwords"
  ],
  "author": {
    "name": "Justin Hall",
    "email": "justin.r.hall@gmail.com"
  },
  "contributors": [],
  "license": "MIT",
  "main": "bin/pwned.js",
  "bin": "bin/pwned.js",
  "directories": {
    "bin": "./bin",
    "lib": "./lib"
  },
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "build": "babel src --out-dir lib --source-maps --ignore \"**/*.test.js\",\"**/__mocks__/\"",
    "clean": "rimraf lib coverage",
    "format": "prettier --write \"**/*.{js,json,md,yml}\"",
    "lint": "eslint .",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "test": "cross-env NODE_ENV=test jest",
    "test:coverage": "npm run test -- --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wKovacs64/pwned.git"
  },
  "bugs": {
    "url": "https://github.com/wKovacs64/pwned/issues"
  },
  "homepage": "https://wkovacs64.github.io/pwned",
  "engines": {
    "node": ">= 6.4.0"
  },
  "dependencies": {
    "hibp": "^7.2.3",
    "ora": "^3.0.0",
    "prettyjson": "^1.2.1",
    "source-map-support": "^0.5.9",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@babel/cli": "7.2.0",
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.2.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "codecov": "3.1.0",
    "cross-env": "5.2.0",
    "eslint": "5.10.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-import": "2.14.0",
    "husky": "1.3.0",
    "jest": "23.6.0",
    "lint-staged": "8.1.0",
    "prettier": "1.15.3",
    "rimraf": "2.6.2"
  }
}
